{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js"
                ],
                "alias": {
                    "_assets": "./src/assets",
                    "_components": "./src/components",
                    "_page": "./src/page",
                    "_styles": "./src/styles",
                    "_utils": "./src/utils",
                    "_context": "./src/context"
                }
            }
        ]
    ]
}